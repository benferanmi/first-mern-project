{
  "routes": [
    {
      "src": "/api/.*",
      "dest": "http://localhost:4080/profile:splat",
      "headers": {
        "Authorization": "mytoken"
      }
    },
    {
      "src": "/user-places/.*",
      "dest": "http://localhost:4000/user-places",
      "headers": {
        "Authorization": "mytoken"
      }
    },
    {
      "src": "/places/.*",
      "dest": "http://localhost:4000/places",
      "headers": {
        "Authorization": "mytoken"
      }
    }
  ]
}
